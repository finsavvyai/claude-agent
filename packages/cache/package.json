    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8"
  },
  "dependencies": {
    "@claude-agent/types": "workspace:*",
    "@claude-agent/shared": "workspace:*",
    "redis": "^4.6.10",
