{
  "name": "@claude-agent/cache",
  "version": "1.0.0",
  "description": "Cache service for the Claude Agent Platform",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./redis": "./src/redis.ts",
    "./memory": "./src/memory.ts",
    "./interfaces": "./src/interfaces.ts"
  },
  "scripts": {
