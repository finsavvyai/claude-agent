# Claude Agent Platform API Environment Configuration

# Application
NODE_ENV=development
PORT=3000

# Database
DATABASE_URL=postgresql://postgres:password@localhost:5432/claude_agent

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# RabbitMQ
RABBITMQ_URL=amqp://localhost:5672

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRES_IN=1d
JWT_REFRESH_SECRET=your-super-secret-refresh-key-change-this-in-production
JWT_REFRESH_EXPIRES_IN=7d

# AI Providers
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key
DEEPSEEK_API_KEY=your-deepseek-api-key
GEMINI_API_KEY=your-gemini-api-key

# RAG Configuration
RAG_EMBEDDING_MODEL=text-embedding-ada-002
VECTOR_STORE_PROVIDER=qdrant
VECTOR_STORE_URL=http://localhost:6333
VECTOR_STORE_API_KEY=
RAG_CACHE_TTL=3600
RAG_CACHE_MAX_SIZE=1000

# Token Optimization
DEFAULT_TOKEN_BUDGET=100.0
TOKEN_ALERT_THRESHOLD=0.8
OPTIMIZATION_STRATEGIES=summarization,compression,selection

# Cloudflare (Optional)
CLOUDFLARE_ACCOUNT_ID=your-cloudflare-account-id
CLOUDFLARE_API_TOKEN=your-cloudflare-api-token
CLOUDFLARE_WORKERS_ENABLED=false
CLOUDFLARE_KV_RAG_CACHE=claude-agent-rag-cache
CLOUDFLARE_KV_TOKEN_CACHE=claude-agent-token-cache

# CORS
CORS_ORIGINS=http://localhost:3000,http://localhost:3001

# Logging
LOG_LEVEL=info
ELASTICSEARCH_URL=http://localhost:9200
ELASTICSEARCH_INDEX=claude-agent-logs

# Monitoring
MONITORING_ENABLED=true
JAEGER_ENDPOINT=http://localhost:14268/api/traces
PROMETHEUS_PORT=9090
